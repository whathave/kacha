/**lista.wxss**/
ul{
  display: flex;
  flex-wrap: wrap;
  margin-left: -2rpx;
}
ul::after{
  content: '';
  flex-grow: 9999999;
}
.item{
  height: 280rpx;
  margin-bottom: 2rpx;
  position: relative;
  flex-grow: 1;
  margin-left: 2rpx;
}
.item a{
  display: block;
  background-size: cover;
  background-position: center center;
  position: relative;
  font-size: 10px;
  width: 100%;
  height: 100%;
}
.item b{
  font-size: 10px;
  position: absolute;
  left: 10rpx;
  top: 10rpx;
  color: #FFF;
  z-index: 99999999;
}